                <div class="inner-border"></div>
                <dt>期間</dt>
                <dd>
                    <div id="reportrange" style="cursor: pointer; padding: 5px 10px; width: 100%">
                        <div class="dates-wrap">
                            <img src="img/calendar.png" alt="calendar" id="calendar1">
                            <div class="start-date">開始日</div>
                            <i class="fa fa-angle-down"></i>
                        </div>
                        <p style="display: inline-block;">~</p>
                        <div class="dates-wrap">
                            <img src="img/calendar.png" alt="calendar" id="calendar2">
                            <div class="end-date">終了日</div>
                            <i class="fa fa-angle-down"></i>
                        </div>
                    </div>
                </dd>
                <div class="inner-border"></div>
                <dt class="agenda-dt">議題</dt>
                <dd><textarea name="agenda" id="agenda" cols="30" rows="10"></textarea></dd>
                <input type="button" id="confirm-btn" value="内容を確認する">
            </dl>
        </div>
    </div>
