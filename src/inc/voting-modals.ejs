<div id="voting-modals">
    <div id="mask"></div>
    <div id="new-first-modal" class="modal">
        <div class="inner-modal">
            <!-- <div class="close-btn"><i class="fas fa-times"></i></div> -->
            <div class="top-text">New Voting</div>
            <div class="inner-border"></div>
            <div id="yn-btn">
                <div class="img-wrap">
                    <img src="img/hand-stop.png" alt="hand">
                </div>
                <p>Yes/No</p>
            </div>
            <div id="select-btn">
                <div class="img-wrap">
                    <img src="img/files.png" alt="papers">
                </div>
                <p>選択式</p>
